/* 头部搜索框 */
.headSearch{width: 100%;height: 100rpx;box-sizing: border-box;padding: 20rpx 30rpx;display: flex;flex-direction: row;align-items: center;justify-content: space-between;top: 0;left: 0;z-index: 10;}

.searchContent{flex: 1;height: 60rpx;background: #fff;border-radius: 50rpx;position: relative;}
.searchContent .icon{position: absolute;left: 26rpx;top: 50%;transform: translateY(-50%);}
.searchContent .input{width: 100%;height: 100%;box-sizing: border-box;padding: 0 140rpx 0 70rpx;font-size: 26rpx;}
.searchContent .btn{width: 120rpx;height: 100%;line-height: 60rpx;position: absolute;top: 0;right: 0;z-index: 10;text-align: center;border-radius: 50rpx;color: #fff;background: #70BB2B;border-radius: 50rpx;font-size: 28rpx;}

.cancel{margin-left: 20rpx;}